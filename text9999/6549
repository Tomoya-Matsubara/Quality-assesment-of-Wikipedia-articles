| rev2 = ''[[Los Angeles Times]]''
| rev2Score = {{Rating|2.5|4}}<ref>{{cite web|last=Baker|first=Soren|date=May 30, 1999|title=Ja Rule; 'Venni Vetti Vecci'; Def Jam; ** 1/2|url=http://articles.latimes.com/1999/may/30/entertainment/ca-42377|work=[[Los Angeles Times]]|publisher=[[Tribune Company]]|accessdate=May 13, 2010}}</ref>
| rev3 = RapReviews
