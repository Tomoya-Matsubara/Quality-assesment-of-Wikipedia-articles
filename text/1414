| rev3 =  [[AbsolutePunk.net]]
| rev3Score =  (87%) <ref>{{cite web |url= http://www.absolutepunk.net/showthread.php?t=205270 |title=Fall Out Boy - Infinity on High - Album Review - AbsolutePunk.net |work=absolutepunk.net |accessdate=9 August 2011}}</ref>
| rev4 =  [[IGN]]
| rev4Score =  (8.3/10)<ref name=IGNReview/>
